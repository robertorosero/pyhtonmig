# NOTE: Makefile.in is converted into Makefile by the configure script
# in the parent directory.  Once configure has run, you can recreate
# the Makefile by running just config.status.

# === Variables set by config.stat ===

srcdir=		@srcdir@
VPATH=		@srcdir@

CC=		@CC@
RANLIB=		@RANLIB@
DEFS=		@DEFS@
LIBOBJS=	@LIBOBJS@
LIBS=		@LIBS@


# === Other things that are customizable but not by configure ===

TOP=		..
INCLDIR=	$(TOP)/Include
OPT=		-g
CFLAGS=		$(OPT) -I$(INCLDIR) $(DEFS)

AR=		ar
MKDEP=		mkdep
SHELL=		/bin/sh


# === Fixed definitions ===

OBJS=		\
		bltinmodule.o \
		ceval.o cgensupport.o compile.o \
		errors.o \
		frozenmain.o \
		getmtime.o graminit.o \
		import.o \
		marshal.o modsupport.o mystrtoul.o \
		pythonmain.o pythonrun.o \
		structmember.o sysmodule.o \
		traceback.o \
		version.o \
		$(LIBOBJS)

LIB=		libPython.a

SYSLIBS=	-lm


# === Rules ===

all:		$(LIB)

$(LIB):		$(OBJS)
		$(AR) cr $(LIB) $(OBJS)
		$(RANLIB) $(LIB)

clean:
		-rm -f *.o core *~ [@,#]* *.old *.orig *.rej

clobber:	clean
		-rm -f *.a tags TAGS

Makefile:	Makefile.in $(TOP)/config.status
		CONFIG_FILES=Makefile CONFIG_HEADERS= \
		$(SHELL) $(TOP)/config.status

depend:
		$(MKDEP) $(CFLAGS) \
			`echo $(OBJS) | tr ' ' '\012' | sed 's/\.o/.c/'`

.PRECIOUS:	Makefile

# DO NOT DELETE THIS LINE -- mkdep uses it.
# DO NOT PUT ANYTHING AFTER THIS LINE, IT WILL GO AWAY.
# IF YOU PUT ANYTHING HERE IT WILL GO AWAY
